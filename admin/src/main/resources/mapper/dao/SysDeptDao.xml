<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.common.data.dao.SysDeptDao">

    <select id="getById" resultType="com.example.common.v0.data.entity.SysGroupEntity">
		select t1.*,(select t2.name from sys_dept t2 where t2.id=t1.pid)parentName from sys_dept t1
		where t1.id = #{value}
	</select>

</mapper>
